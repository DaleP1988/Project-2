{{> navbars }}
<main id="preview-area" class="scrollbar scrollbar-primary">
<h4 class="center title preview-group-title"></h4>
<hr class="title-hr center">
<div class="row prev-and-form">
    <div class="edit-div col s12 m6">
        <h6 class="center title">Form</h6>
        <div class="row">
            <form class="col s11">
                <div class="row">
                    <div class="input-field col s12">
                        <input placeholder="URL only" id="logo-input" type="text" class="validate">
                        <label for="logo-input">Logo</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s12">
                        <input placeholder="URL only" id="main-image" type="text" class="validate">
                        <label for="main-image">Main image</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s12">
                        <input id="name-input" type="text" class="validate">
                        <label for="name-input">Name</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s12">
                        <input id="position-input" type="text" class="validate">
                        <label for="position-input">Position</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s12">
                        <input id="telephone" type="text" class="validate">
                        <label for="telephone">Telephone</label>
                    </div>
                </div>
                <div class="row">
                    <div class="input-field col s12">
                        <input id="email-input" type="text" class="validate">
                        <label for="profile-image">Email</label>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div class="prev-div col s12 m6">
        <div class="row">
            <div id="temp-area-prev" class="col s11">
                
            </div>
        </div>
    </div>
</div>
</main>

{{> bottom }}